{
  "name": "react-eslint-prettier-husky-example",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.6",
    "@types/react-redux": "^7.1.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.38.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.0",
    "scss": "^0.2.4",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "CI=true react-scripts test",
    "eject": "react-scripts eject",
    "lint": "yarn lint:ts",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "lint-fix": "yarn lint:ts:fix",
    "lint:ts": "eslint . --ext .ts,.tsx,.cjs,.mjs --ignore-path .gitignore --ignore-path .eslintignore",
    "lint:ts:fix": "eslint . --ext .ts,.tsx,.cjs,.mjs --fix --ignore-path .gitignore --ignore-path .eslintignore"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "typescript": "^4.4.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint:ts:fix",
      "prettier --write .",
      "git add ."
    ],
    "*.js": "eslint --cache --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
